<h2>Form to edit this dino</h2>
<form action="/dinos/<%= dino.id %>/?_method=PUT" method="POST">
    <label for="dino-name">Name:</label>
    <input type="text" name="name" id="dino-name" placeholder="Dino Name" value="<%= dino.name %>" />
    <label for="dino-type">Dino Type:</label>
    <input type="text" name="type" id="dino-type" value="<%= dino.type %>"/>
    <input type="submit" value="Edit that dino!" class="btn"/>
</form>